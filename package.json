{
  "name": "indexeddb-chunk-store",
  "version": "2.1.1",
  "description": "A Chunk Store compatible store backed by IndexedDB",
  "main": "index.js",
  "scripts": {
    "build": "standard && browserify -r . -s IdbChunkStore | uglifyjs -m > idbchunkstore.min.js",
    "standard": "standard",
    "test": "standard && zuul -- test.js",
    "test-local": "standard && zuul --local -- test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xuset/indexeddb-chunk-store.git"
  },
  "keywords": [
    "abstract-chunk-store",
    "chunk-store",
    "idb-chunk-store",
    "chunk",
    "store",
    "indexdb",
    "indexeddb",
    "idb"
  ],
  "author": "xuset",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xuset/indexeddb-chunk-store/issues"
  },
  "homepage": "https://github.com/xuset/indexeddb-chunk-store#readme",
  "dependencies": {
    "idb-kv-store": "^4.0.0"
  },
  "devDependencies": {
    "abstract-chunk-store": "^1.3.0",
    "browserify": "^13.3.0",
    "run-parallel": "^1.1.6",
    "standard": "^8.6.0",
    "tape": "^4.6.3",
    "uglify-js": "^2.7.5",
    "zuul": "^3.11.1"
  }
}
